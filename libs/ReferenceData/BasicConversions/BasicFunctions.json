{
  "Languages": [
    "MLP",
    "LaTeX",
    "Maple",
    "Mathematica"
  ],
  "Functions": {
    "Fraction": {
      "MLP": "fraction",
      "LaTeX": "\\frac{$0}{$1}",
      "Maple": "($0)/($1)",
      "Mathematica": "Divide[$0,$1]"
    },
    "Binomial": {
      "MLP": "binomial coefficient",
      "LaTeX": "\\binom{$0}{$1}",
      "Maple": "binomial($0,$1)",
      "Mathematica": "Binomial[$0,$1]"
    },
    "Square Root": {
      "MLP": "square root",
      "LaTeX": "\\sqrt{$0}",
      "Maple": "sqrt($0)",
      "Mathematica": "Sqrt[$0]"
    },
    "Surd Root": {
      "MLP": "radical with a specified index",
      "LaTeX": "\\sqrt[$0]{$1}",
      "Maple": "surd($1,$0)",
      "Mathematica": "Surd[$1,$0]"
    },
    "Factorial": {
      "MLP": "exclamation point",
      "LaTeX": "$0!",
      "Maple": "factorial($0)",
      "Mathematica": "($0)!"
    },
    "Double Factorial": {
      "MLP": "double exclamation point",
      "LaTeX": "$0!!",
      "Maple": "doublefactorial($0)",
      "Mathematica": "($0)!!"
    },
    "Modulo": {
      "MLP": "modulo",
      "LaTeX": "$0 \\mod $1",
      "Maple": "`modp`($0,$1)",
      "Mathematica": "Mod[$0,$1]"
    },
    "Subscript": {
      "MLP": "underscore",
      "LaTeX": "$0_{$1}",
      "Maple": "$0[$1]",
      "Mathematica": "Subscript[$0, $1]"
    },
    "Exponential": {
      "MLP": "exponential",
      "LaTeX": "\\expe",
      "Maple": "exp($0)",
      "Mathematica": "Exp[$0]"
    },
    "Absolute Value": {
      "MLP": "absolute value",
      "LaTeX": "| $0 |",
      "Maple": "abs($0)",
      "Mathematica": "Abs[$0]"
    },
    "Sum": {
      "MLP": "sum",
      "LaTeX": "\\sum_{$0 = $1}^{$2}{$3}",
      "Maple": "sum($3, $0 = $1..$2)",
      "Mathematica": "Sum[$3, {$0, $1, $2}]"
    },
    "SumSet": {
      "MLP": "sumSet",
      "LaTeX": "\\sum_{$0 \\in $1}{$2}",
      "Maple": "sum($2, $0 in $1)",
      "Mathematica": "Sum[$2, {$0, $1}]"
    },
    "Product": {
      "MLP": "prod",
      "LaTeX": "\\prod_{$0 = $1}^{$2}{$3}",
      "Maple": "product($3, $0 = $1..$2)",
      "Mathematica": "Product[$3, {$0, $1, $2}]"
    },
    "ProductSet": {
      "MLP": "prodSet",
      "LaTeX": "\\prod_{$0 \\in $1}{$2}",
      "Maple": "product($2, $0 in $1)",
      "Mathematica": "Product[$2, {$0, $1}]"
    },
    "Integral": {
      "MLP": "int",
      "LaTeX": "\\int_{$1}^{$2}{$3} \\diff{$0}",
      "Maple": "int($3, $0 = $1..$2)",
      "Mathematica": "Integrate[$3, {$0, $1, $2}]"
    },
    "IntegralIndef": {
      "MLP": "intIndef",
      "LaTeX": "\\int{$1} \\diff{$0}",
      "Maple": "int($1, $0)",
      "Mathematica": "Integrate[$1, $0]"
    },
    "Derivative": {
      "MLP": "derivative",
      "LaTeX": "\\deriv[$2]{$0}{$1}",
      "Maple": "subs( temp=$1, diff( $0, temp$($2) ) )",
      "Mathematica": "(D[$0, {temp, $2}]/.temp-> $1)"
    },
    "Limit": {
      "MLP": "lim",
      "LaTeX": "\\lim_{$0 \\to $1}{$2}",
      "Maple": "limit($2, $0 = $1)",
      "Mathematica": "Limit[$2, $0 -> $1]"
    },
    "LimitLeft": {
      "MLP": "limleft",
      "LaTeX": "\\lim_{$0 \\to $1^{-}}{$2}",
      "Maple": "limit($2, $0 = $1, left)",
      "Mathematica": "Limit[$2, $0 -> $1, Direction -> \"FromBelow\"]"
    },
    "LimitRight": {
      "MLP": "limright",
      "LaTeX": "\\lim_{$0 \\to $1^{+}}{$2}",
      "Maple": "limit($2, $0 = $1, right)",
      "Mathematica": "Limit[$2, $0 -> $1, Direction -> \"FromAbove\"]"
    },
    "LoadPackage": {
      "MLP": "load-package",
      "LaTeX": "",
      "Maple": "with($0)",
      "Mathematica": ""
    },
    "UnLoadPackage": {
      "MLP": "unload-package",
      "LaTeX": "",
      "Maple": "unwith($0)",
      "Mathematica": ""
    }
  },
